/*                                 MACROS                                     */
/* ========================================================================== */

/* Macros send a sequence of keycodes or commands in order when triggered  */
/* They're like recording and playing back a series of keypresses  */
    macros {

        /* ------------------------------------------------------------------ */
        /*                       System Macros                                */
        /* ------------------------------------------------------------------ */

        rgb_ug_status_macro: rgb_ug_status_macro {
